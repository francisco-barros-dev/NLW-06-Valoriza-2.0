{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es2019",
    "module": "commonjs",
    "allowJs": true,
    "rootDir": "src",
    "strict": false,
    "baseUrl": "src",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true
  },
  "include": ["src/**/*"],
  "exclude": [""]
}
